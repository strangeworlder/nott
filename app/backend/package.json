		"express": "^4.18.2",
		"express-rate-limit": "^7.1.5",
		"express-validator": "^7.0.1",
		"helmet": "^7.1.0",
		"ioredis": "^5.3.2",
		"joi": "^17.11.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"nodemailer": "^6.9.7",
		"pg": "^8.11.3",
		"redis": "^4.6.10",
		"sharp": "^0.33.0",
		"simple-peer": "^9.11.1",
		"socket.io": "^4.7.4",
		"socket.io-redis": "^6.1.1",
