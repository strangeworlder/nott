{
	"name": "nott-backend",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "nodemon --exec tsx src/server.ts",
		"build": "tsc",
		"start": "node dist/server.js",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
